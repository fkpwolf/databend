DROP DATABASE IF EXISTS ss2;

statement ok
CREATE DATABASE ss;

statement ok
CREATE DATABASE ss1;

statement ok
CREATE DATABASE ss2;

statement query T 
SHOW DATABASES like 'ss%';

----
ss
ss1
ss2

statement query TT
SHOW FULL DATABASES like 'ss%';

----
default ss
default ss1
default ss2

statement query TT
SHOW FULL DATABASES FROM default like 'ss%';

----
default ss
default ss1
default ss2

statement query T 
SHOW SCHEMAS like 'ss%';

----
ss
ss1
ss2

