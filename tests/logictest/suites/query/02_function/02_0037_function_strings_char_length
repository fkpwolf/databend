statement query T
SELECT CHAR_LENGTH(NULL);

----
NULL

statement query I
SELECT CHAR_LENGTH('');

----
0

statement query I
SELECT CHAR_LENGTH('hello world');

----
11

statement query I
SELECT CHAR_LENGTH('早ab');

----
3

