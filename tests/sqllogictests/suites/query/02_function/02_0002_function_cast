query T
SELECT typeof(CAST(number AS float)) FROM numbers_mt(1)
----
FLOAT

query T
SELECT typeof(CAST(number AS float32)) FROM numbers_mt(1)
----
FLOAT

query T
SELECT typeof(CAST(number AS UInt64)) FROM numbers_mt(1)
----
BIGINT UNSIGNED

